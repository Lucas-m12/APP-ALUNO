{
	"expo": {
		"name": "Sagecam",
		"slug": "Sagecam",
		"privacy": "public",
		"sdkVersion": "36.0.0",
		"platforms": ["ios", "android"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/img/icon.png",
		"splash": {
			"image": "./assets/img/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#5DBCD2"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "campestre.praxisescola.com.br"
		},
		"android": {
			"package": "campestre.praxisescola.com.br"
		}
	}
}
